{
  "crons": [
    {
      "path": "/api/generate",
      "schedule": "0 7 * * *"
    }
  ],
  "functions": {
    "api/generate": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}