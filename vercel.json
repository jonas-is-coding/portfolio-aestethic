{
  "crons": [
    {
      "path": "/api/generate",
      "schedule": "0 7 * * *"
    }
  ],
  "functions": {
    "app/api/generate/**/route.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}